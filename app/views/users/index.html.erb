<p id="notice"><%= notice %></p>

<h1>登録ユーザー</h1>

<table>
  <thead>
    <tr>
      <th>姓</th>
      <th>名</th>
      <th>メールアドレス</th>
      <th>性別</th>
      <th>住所</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.last_name %></td>
        <td><%= user.first_name %></td>
        <td><%= user.email %></td>
        <td><%= user.sex %></td>
        <td><%= user.adress %></td>
        <td><%= link_to '詳細', user %></td>
        <!--<td><%= link_to '編集', edit_user_path(user) %></td>-->
        <!--<td><%= link_to '削除', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<% if @current_user == nil %>
  <%= link_to '新規ユーザー登録', new_user_path %>
<% end %>
