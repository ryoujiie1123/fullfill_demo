<p id="notice"><%= notice %></p>
<div class="seets-table">
<h1>お店を探す</h1>
<table>
  <thead>
    <tr>
      <th>店舗名</th>
      <th>タイトル</th>
      <th>残席数</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seets.each do |seet| %>
      <tr>
        <td><%= link_to(seet.client.name, "/seets/#{seet.client.id}") %></td>
        <td><%= link_to(seet.client.title, "/seets/#{seet.client.id}") %></td>
        <td><%= seet.remain_seat %></td>
        <td><%= link_to '詳細', seet %></td>
        <td><%= link_to '編集', edit_seet_path(seet) %></td>
        <td><%= link_to '削除', seet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '席数登録', new_seet_path %>
</div>