<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>店舗ID</th>
      <th>店舗名</th>
      <th>タイトル</th>
      <th>ユーザーID</th>
      <th>ユーザー名</th>
      <th>Seet ID</th>
      <th>席数</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @takings.each do |taking| %>
      <tr>
        <td><%= taking.client_id %></td>
        <td><%= link_to(taking.client.name, "/takings/#{taking.client.id}") %></td>
        <td><%= link_to(taking.client.title, "/seets/#{taking.client.id}") %></td>
        <td><%= taking.user_id %></td>
        <td><%= link_to(taking.user.last_name+taking.user.first_name, "/users/#{taking.user.id}") %></td>
        <td><%= taking.seet_id %></td>
        <td><%= taking.seet_number %></td>
        <td><%= link_to 'Show', taking %></td>
        <td><%= link_to 'Edit', edit_taking_path(taking) %></td>
        <td><%= link_to 'Destroy', taking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taking', new_taking_path %>
